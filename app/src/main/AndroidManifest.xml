<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.yantu.androidproject">

    <uses-permission android:name="android.permission.VIBRATE" />
    <application
        android:allowBackup="true"
        android:icon="@mipmap/icon_main"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/icon_main"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".TomatoClockActivity"></activity>
        <activity android:name=".LaunchActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".EditHobbyActivity" />
        <activity android:name=".HobbyDetailAvtivity" />

        <receiver android:name=".Broadcast.AlarmReceiver">
            <intent-filter>
                <action android:name="intent_alarm_log" />
            </intent-filter>
        </receiver>

        <activity android:name=".MainActivity" />
        <activity android:name=".popAddActivity" />
    </application>

</manifest>